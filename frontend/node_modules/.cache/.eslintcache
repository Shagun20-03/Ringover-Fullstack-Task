 else {\r\n      setFilters((prev) => ({\r\n        ...prev,\r\n        colors: [...filters.colors, getColorByIndex(i)],\r\n      }));\r\n    }\r\n  };\r\n\r\n  useEffect(() => {\r\n    handleFilters();\r\n  }, [filters]);\r\n\r\n  const handleFilterCheckbox = ({ target }) => {\r\n    const { name, value } = target;\r\n    if (filters[name]?.includes(value)) {\r\n      const newValues = filters[name].filter((v) => v !== value);\r\n      setFilters((prev) => ({ ...prev, [name]: newValues }));\r\n    } else {\r\n      setFilters((prev) => ({ ...prev, [name]: [...filters[name], value] }));\r\n    }\r\n    handleFilters();\r\n  };\r\n\r\n  const handleReset = () => {\r\n    setFilters(INITIAL_STATE);\r\n    dispatch(ResetFilters({}));\r\n  };\r\n\r\n  return (\r\n    <div className=\"filters\">\r\n      <form className=\"wrapper\" onSubmit={handleFilters}>\r\n        <div className=\"top\">\r\n          <h2>Filters</h2>\r\n          <img src=\"/assets/filter.png\" alt=\"filter\" />\r\n        </div>\r\n        <div className=\"cost\">\r\n          <h2>Cost</h2>\r\n          <div className=\"field\">\r\n            <input\r\n              type=\"checkbox\"\r\n              id=\"p1\"\r\n              value=\"1500-4000\"\r\n              name=\"prices\"\r\n              onChange={handleFilterCheckbox}\r\n            />\r\n            <label htmlFor=\"p1\">Rs. 1500-4000</label>\r\n          </div>\r\n          <div className=\"field\">\r\n            <input\r\n              type=\"checkbox\"\r\n              id=\"p2\"\r\n              value=\"4001-7000\"\r\n              name=\"prices\"\r\n              onChange={handleFilterCheckbox}\r\n            />\r\n            <label htmlFor=\"p2\">Rs. 4001-7000</label>\r\n          </div>\r\n          <div className=\"field\">\r\n            <input\r\n              type=\"checkbox\"\r\n              id=\"p3\"\r\n              value=\"7001-99999\"\r\n              name=\"prices\"\r\n              onChange={handleFilterCheckbox}\r\n            />\r\n            <label htmlFor=\"p3\">Rs. 7001+</label>\r\n          </div>\r\n        </div>\r\n        <div className=\"color\">\r\n          <h2>Color</h2>\r\n          <div className=\"container\">\r\n            {Array(5)\r\n              .fill()\r\n              .map((_, i) => (\r\n                <span\r\n                  key={i}\r\n                  className={\r\n                    filters.colors?.includes(getColorByIndex(i))\r\n                      ? \"active\"\r\n                      : null\r\n                  }\r\n                  onClick={() => handleColor(i)}\r\n                ></span>\r\n              ))}\r\n          </div>\r\n        </div>\r\n        <div className=\"designTemplates\">\r\n<h2>Design Templates</h2>\r\n          <div className=\"field\">\r\n            <input\r\n              type=\"checkbox\"\r\n              value={2}\r\n              onChange={handleFilterCheckbox}\r\n              name=\"designTemplates\"\r\n              id=\"dt1\"\r\n            />\r\n            <label htmlFor=\"dt1\">2</label>\r\n          </div>\r\n          <div className=\"field\">\r\n            <input\r\n              type=\"checkbox\"\r\n              value={3}\r\n              onChange={handleFilterCheckbox}\r\n              name=\"designTemplates\"\r\n              id=\"dt2\"\r\n            />\r\n            <label htmlFor=\"dt2\">3</label>\r\n          </div>\r\n          <div className=\"field\">\r\n            <input\r\n              type=\"checkbox\"\r\n              value={4}\r\n              onChange={handleFilterCheckbox}\r\n              name=\"designTemplates\"\r\n              id=\"dt3\"\r\n            />\r\n            <label htmlFor=\"dt3\">3+</label>\r\n          </div>\r\n        </div>\r\n        <div className=\"type\">\r\n          <h2>Type</h2>\r\n          <div className=\"field\">\r\n            <input\r\n              type=\"checkbox\"\r\n              id=\"t1\"\r\n              value=\"loafers\"\r\n            